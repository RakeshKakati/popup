{
  "version": 2,
  "builds": [
    {
      "src": "backend-example/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "landing/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend-example/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "landing/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

